    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./compiled/index.js"
  },
  "author": "Gleb Nikitin and Doogls",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^7.32.0",
    "typescript": "^4.5.4"
  },
  "build": {
    "productName": "Barbaris",
    "electronVersion": "12.0.5",
    "win": {
